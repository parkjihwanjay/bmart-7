{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "prisma-sync": "npx prisma introspect --experimental-reintrospection && npx prisma generate",
    "prisma-sync:dev": "dotenv -e .env.dev -- cross-var wait-on tcp:%DB_PORT% && npm run prisma-sync",
    "generate": "npx prisma generate",
    "studio": "npx prisma studio --experimental",
    "db-forward": "dotenv -e .env.dev -- cross-var \"ssh -i %PEM_PATH% -L %DB_PORT%:localhost:%DB_PORT% %SERVER_USER%@%SERVER_HOST%\"",
    "build:watch": "NODE_ENV=dev nodemon --exec ts-node server.ts --watch ./ -e ts",
    "dev": "npm-run-all --parallel db-forward build:watch",
    "start": "NODE_ENV=prod ts-node server.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7",
    "typescript-eslint": "0.0.1-alpha.0",
    "wait-on": "^5.2.0"
  },
  "dependencies": {
    "@prisma/cli": "^2.5.0",
    "@prisma/client": "^2.5.0",
    "apollo-server-express": "^2.16.1",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.0.18",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.8",
    "path": "^0.12.7",
    "query-parser-url": "^1.0.4",
    "ts-node": "^8.10.2"
  }
}
